options = Options()

# ðŸ”¹ Attach to already opened enterprise Chrome
options.add_experimental_option("debuggerAddress", "127.0.0.1:9222")

driver = webdriver.Chrome(options=options)

print("âœ… Connected to Corporate Chrome")
print(driver.title)


